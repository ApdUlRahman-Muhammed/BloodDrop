<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/Donor_list"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="#FFFF"
	android:orientation="vertical"
	tools:context="com.example.android.blooddrop.Patient">

	<FrameLayout
		android:id="@+id/donors_container"
		android:layout_width="match_parent"
		android:layout_height="740dp">

	</FrameLayout>

	<TextView
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginBottom="5dp"
		android:gravity="center"
		android:text="@string/click_on_google_map_image_to_see_donor_s_location"
		android:textSize="25sp" />

	<LinearLayout
		android:layout_width="500dp"
		android:layout_height="100dp"
		android:layout_gravity="center"
		android:layout_marginStart="10dp"
		android:layout_marginTop="45dp"
		android:orientation="horizontal">

		<ImageView
			android:id="@+id/googleMapId"
			android:layout_width="200dp"
			android:layout_height="75dp"
			android:layout_gravity="bottom"
			android:layout_marginBottom="10dp"
			android:contentDescription="@string/google_map_image"
			android:scaleType="centerCrop"
			app:srcCompat="@drawable/googlemap" />

	</LinearLayout>
</LinearLayout>